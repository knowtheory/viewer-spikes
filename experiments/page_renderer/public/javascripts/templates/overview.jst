<div class="page_mark ui-slider-handle">
  <div class="mark_text">page <span class="page_number">1</span></div>
</div>
<% pages.each(function(page, pageIndex) { 
  var pageCount  = pages.length;
%>
<div class="page<%= (pageIndex % 2) ? ' even' : '' %>" style="width: 100%;position: absolute; top:<%= (100*pageIndex/pageCount) %>%; height:<%= (100/pageCount) %>%"></div>
<% }); %>
